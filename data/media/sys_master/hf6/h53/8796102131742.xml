<editor>

	<group qualifier="general" visible="true" initially-opened="true">
		<label key="cockpit.config.label.General" />
		<property qualifier="pricerow.product"/>
		<property qualifier="pricerow.minqtd"/>
		<property qualifier="pricerow.price"/>
		<property qualifier="pricerow.currency"/>
		<property qualifier="pricerow.channel"/>
