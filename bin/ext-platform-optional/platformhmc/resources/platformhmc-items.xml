    <attribute qualifier="includingProducts" type="java.lang.Boolean">
     <modifiers read="true" write="true" optional="false"/>
     <defaultvalue>Boolean.FALSE</defaultvalue>
     <persistence type="property"/>
    </attribute>

    <!-- tab 3: select products -->

    <attribute qualifier="products" type="SyncProductList">
     <modifiers read="true" write="true" optional="true"/>
     <persistence type="property"/>
    </attribute>

    <!-- tab 4: select medias -->

    <attribute qualifier="medias" type="SyncMediaList">
     <modifiers read="true" write="true" optional="true"/>
     <persistence type="property"/>
    </attribute>

    <!-- tab 5: select keywords -->

    <attribute qualifier="keywords" type="SyncKeywordList">
     <modifiers read="true" write="true" optional="true"/>
     <persistence type="property"/>
    </attribute>

    <!-- tab 7: select others -->

    <attribute qualifier="others" type="SyncOthersList">
     <modifiers read="true" write="true" optional="true"/>
     <persistence type="property"/>
    </attribute>

    <!-- tab 6: target versions -->
    <attribute qualifier="ignoreErrors" type="java.lang.Boolean">
     <modifiers read="true" write="true" optional="false"/>
     <persistence type="property"/>
     <defaultvalue>Boolean.TRUE</defaultvalue>
    </attribute>
    <attribute qualifier="forceUpdate" type="java.lang.Boolean">
     <modifiers read="true" write="true" optional="false"/>
     <persistence type="property"/>
     <defaultvalue>Boolean.FALSE</defaultvalue>
    </attribute>
    <attribute qualifier="keepCronJob" type="java.lang.Boolean">
     <modifiers read="true" write="true" optional="false"/>
     <defaultvalue>Boolean.TRUE</defaultvalue>
     <persistence type="property"/>
    </attribute>
          <attribute qualifier="executableJobs" type="SyncItemJobCollection">
     <modifiers read="true" write="false" optional="true"/>
     <persistence type="property"/>
    </attribute>
    <attribute qualifier="logToFile" type="java.lang.Boolean">
     <modifiers read="true" write="true" initial="false" optional="false"/>
     <persistence type="property"/>
     <defaultvalue>Boolean.TRUE</defaultvalue>
    </attribute>
    <attribute qualifier="logToDatabase" type="java.lang.Boolean">
     <modifiers read="true" write="true" initial="false" optional="false"/>
     <persistence type="property"/>
     <defaultvalue>Boolean.FALSE</defaultvalue>
    </attribute>
    <attribute qualifier="logLevelFile" type="JobLogLevel">
     <modifiers read="true" write="true" initial="false" optional="false"/>
     <persistence type="property"/>
     <defaultvalue>em().getEnumerationValue("JobLogLevel", "INFO")</defaultvalue>
    </attribute>
    <attribute qualifier="logLevelDatabase" type="JobLogLevel">
     <modifiers read="true" write="true" initial="false" optional="false"/>
     <persistence type="property"/>
     <defaultvalue>em().getEnumerationValue("JobLogLevel", "WARNING")</defaultvalue>
