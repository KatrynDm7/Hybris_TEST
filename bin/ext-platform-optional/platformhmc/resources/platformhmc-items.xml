<itemtype code="CompareCatalogVersionWizard"
      extends="WizardBusinessItem"
      jaloonly="true"
      jaloclass="de.hybris.platform.catalog.hmc.CompareCatalogVersionWizard"
      generate="true" autocreate="true" >
   <attributes>

    <!-- essential infos -->
    <attribute qualifier="infoAboutComparedCatalogVersions" type="java.lang.String">
     <modifiers read="true" write="false" optional="false"/>
     <persistence type="jalo"/>
    </attribute>

    <!-- tab 1: catalog settings -->

    <attribute qualifier="sourceVersion" type="CatalogVersion">
     <modifiers read="true" write="true" optional="false"/>
     <persistence type="property"/>
    </attribute>
           <attribute qualifier="targetVersion" type="CatalogVersion">
     <modifiers read="true" write="true" optional="false"/>
     <persistence type="property"/>
    </attribute>
    <attribute qualifier="overwriteApprovalStatus" type="java.lang.Boolean">
     <modifiers read="true" write="true" optional="false"/>
     <persistence type="property"/>
     <defaultvalue>Boolean.TRUE</defaultvalue>
    </attribute>
    <attribute qualifier="keepCronJob" type="java.lang.Boolean">
     <modifiers read="true" write="true" optional="false"/>
     <defaultvalue>Boolean.TRUE</defaultvalue>
     <persistence type="property"/>
    </attribute>
    <attribute qualifier="priceTolerance" type="java.lang.Double">
     <modifiers read="true" write="true" optional="false"/>
     <persistence type="property"/>
     <defaultvalue>new Double( 5 )</defaultvalue>
    </attribute>
    <attribute qualifier="priceCustomer" type="Customer">
     <modifiers read="true" write="true" optional="true"/>
     <persistence type="property"/>
    </attribute>

    <!-- tab 3: summary: new and missing products, price differences -->
    <attribute qualifier="missingProducts" type="java.lang.Integer">
     <modifiers read="true" write="false" optional="false"/>
     <persistence type="jalo"/>
    </attribute>
    <attribute qualifier="newProducts" type="java.lang.Integer">
     <modifiers read="true" write="false" optional="false"/>
     <persistence type="jalo"/>
    </attribute>
    <attribute qualifier="overviewOutOfToleranceProducts" type="java.lang.Integer">
     <modifiers read="true" write="true" optional="true"/>
     <persistence type="property"/>
    </attribute>

    <!-- n/a -->

    <!-- fill in pre-settings for <shortcut name="find.differences.product.NEW> -->
    <attribute qualifier="mode1" type="ProductDifferenceMode">
              <modifiers read="true" write="true" search="true" optional="true" initial="false"/>
            <persistence type="property"/>
    </attribute>
    <!-- fill in pre-settings for <shortcut name="find.differences.product.PRICE_DIFFERENCES> -->
    <attribute qualifier="mode2" type="ProductDifferenceMode">
              <modifiers read="true" write="true" search="true" optional="true" initial="false"/>
            <persistence type="property"/>
    </attribute>
    <!-- fill in pre-settings for <shortcut name="find.differences.product.REMOVED> -->
    <attribute qualifier="mode3" type="ProductDifferenceMode">
              <modifiers read="true" write="true" search="true" optional="true" initial="false"/>
            <persistence type="property"/>
    </attribute>
    <attribute qualifier="cronJob" type="CompareCatalogVersionsCronJob">
            <modifiers read="true" write="true" search="true" optional="true" initial="false"/>
            <persistence type="property"/>
    </attribute>
   </attributes>
  </itemtype>


  <itemtype code="RemoveCatalogWizard"
      extends="WizardBusinessItem"
      jaloonly="true"
      jaloclass="de.hybris.platform.catalog.hmc.RemoveCatalogWizard"
      generate="true" autocreate="true" >
   <attributes>

    <!-- tab 1: catalog selection -->
    <attribute qualifier="catalog" type="Catalog">
     <modifiers write="true" optional="false"/>
     <persistence type="jalo"/><!-- delegated to cronjob -->
