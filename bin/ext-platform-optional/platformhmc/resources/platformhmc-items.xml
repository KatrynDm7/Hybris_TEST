     <modifiers write="false" optional="true"/>
     <persistence type="jalo"/>
    </attribute>
    <attribute qualifier="availableCatalogVersions" type="CatalogVersionCollection">
     <modifiers write="false" optional="true"/>
     <persistence type="jalo"/>
    </attribute>
    <attribute qualifier="cronJob" type="RemoveCatalogVersionCronJob">
     <modifiers write="false"/>
     <persistence type="jalo"/>
    </attribute>
    <attribute qualifier="notRemovedItemsMedia" type="ImpexMedia">
     <modifiers write="false"/>
     <persistence type="jalo"/>
    </attribute>

   </attributes>
  </itemtype>
  
  <itemtype code="SynchronizeCatalogVersionWizard"
      extends="WizardBusinessItem"
      jaloonly="true"
      jaloclass="de.hybris.platform.catalog.hmc.SynchronizeCatalogVersionWizard"
      generate="true" autocreate="true" >
   <attributes>

    <attribute qualifier="presetItems" type="ItemCollection">
     <modifiers read="true" write="true" optional="true" initial="false" private="true"/>
     <persistence type="property"/>
    </attribute>

    <!-- tab 1: source version -->

    <attribute qualifier="sourceVersions" type="CatalogVersionCollection">
     <modifiers read="true" write="true" optional="true"/>
     <persistence type="property"/>
    </attribute>

    <!-- tab 2: select categories -->
    <attribute qualifier="categories" type="SyncCategoryList">
     <modifiers read="true" write="true" optional="true"/>
     <persistence type="property"/>
    </attribute>
    <attribute qualifier="includingSubcategories" type="java.lang.Boolean">
     <modifiers read="true" write="true" optional="false"/>
