    <!-- tab 2: catalog version selection -->
    <attribute qualifier="catalogVersion" type="CatalogVersion">
     <modifiers write="true" optional="true"/>
     <persistence type="jalo"/><!-- delegated to cronjob -->
    </attribute>
    <attribute qualifier="dontRemoveObjects" type="java.lang.Boolean">
     <modifiers write="true" optional="false"/>
     <persistence type="jalo"/><!-- delegated to cronjob -->
    </attribute>

    <!-- helper attributes -->
