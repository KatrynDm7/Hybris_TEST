<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="abstractInstoreDataImportEventListener"
          class="com.hybris.instore.yinstoreinitialdata.events.AbstractInstoreDataImportEventListener" abstract="true"
          parent="abstractEventListener">
        <property name="setupImpexService" ref="setupImpexService"/>
    </bean>

    <bean id="instoreOnCoreDataImportEventListener"
          class="com.hybris.instore.yinstoreinitialdata.events.InstoreOnCoreDataImportEventListener"
          parent="abstractInstoreDataImportEventListener"/>

    <bean id="instoreOnSampleDataImportEventListener"
          class="com.hybris.instore.yinstoreinitialdata.events.InstoreOnSampleDataImportEventListener"
          parent="abstractInstoreDataImportEventListener"/>

</beans>