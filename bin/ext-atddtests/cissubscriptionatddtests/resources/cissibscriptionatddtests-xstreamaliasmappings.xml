<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config />
	
 	<bean class="de.hybris.platform.commercefacades.xstream.alias.TypeAliasMapping">
		<property name="alias" value="subscriptionorder" />
		<property name="aliasedClass" value="com.hybris.cis.api.subscription.model.CisSubscriptionOrder" />
	</bean> 

 	<bean class="de.hybris.platform.commercefacades.xstream.alias.TypeAliasMapping">
		<property name="alias" value="item" />
		<property name="aliasedClass" value="com.hybris.cis.api.subscription.model.CisSubscriptionItem" />
	</bean> 

 	<bean class="de.hybris.platform.commercefacades.xstream.alias.TypeAliasMapping">
		<property name="alias" value="charge" />
		<property name="aliasedClass" value="com.hybris.cis.api.subscription.model.CisChargeEntry" />
	</bean> 

 	<bean class="de.hybris.platform.commercefacades.xstream.alias.AttributeAliasMapping">
 		<property name="alias" value="id" />
		<property name="attributeName" value="id" />
		<property name="aliasedClass" value="com.hybris.cis.api.subscription.model.CisSubscriptionOrder" />
	</bean>

 	<bean class="de.hybris.platform.commercefacades.xstream.alias.AttributeAliasMapping">
		<property name="alias" value="id" />
		<property name="attributeName" value="id" />
		<property name="aliasedClass" value="com.hybris.cis.api.subscription.model.CisSubscriptionItem" />
	</bean> 

 	<bean class="de.hybris.platform.commercefacades.xstream.alias.AttributeAliasMapping">
		<property name="alias" value="id" />
		<property name="attributeName" value="id" />
		<property name="aliasedClass" value="com.hybris.cis.api.subscription.model.CisSubscriptionPlan" />
	</bean> 

</beans>	