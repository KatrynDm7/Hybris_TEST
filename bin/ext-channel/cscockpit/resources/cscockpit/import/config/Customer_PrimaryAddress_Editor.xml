<?xml version="1.0" encoding="UTF-8"?>
<editor>
	<group qualifier="DefaultAddress" visible="true" initially-opened="true">
		<label key="cockpit.config.label.General" />
		<property qualifier="Address.title"/>
		<property qualifier="Address.firstname"/>
		<property qualifier="Address.lastname"/>
		<property qualifier="Address.dateofbirth">
			<parameter>
				<name>dateFormat</name>
				<value>medium</value>
			</parameter>
		</property>

		<property qualifier="Address.cellphone"/>
		<property qualifier="Address.email"/>
		<property qualifier="Address.phone1"/>
		<property qualifier="Address.phone2"/>

		<!-- mandatory -->
		<property qualifier="Address.unloadingAddress" visible="false"/>
		<property qualifier="Address.billingAddress" visible="false"/>
		<property qualifier="Address.shippingAddress" visible="false"/>
		<property qualifier="Address.contactAddress" visible="false"/>
		<property qualifier="Item.owner" visible="false"/>
	</group>
</editor>
