<advanced-search>

	<group name="root" type="ImportCockpitCronJob" >
		<label key="config.general.searchfor" />
		<group name="general">
			<label key="cockpit.config.label.General" />
			<property qualifier="ImportCockpitCronJob.code" sort-enabled="true">
				<conditions mode="append">
					<condition operator="between" index="1"/>
				</conditions>
			</property>
			<property qualifier="ImportCockpitCronJob.status" sort-enabled="true" visible="true"/>
			<property qualifier="ImportCockpitCronJob.mapping" visible="false"/>
			<property qualifier="ImportCockpitCronJob.job" visible="false"/>
			<property qualifier="ImportCockpitCronJob.active" visible="false"/>
			<property qualifier="ImportCockpitCronJob.timeTable" visible="false"/>
			<property qualifier="ImportCockpitCronJob.startTime" visible="false"/>
			<property qualifier="ImportCockpitCronJob.endTime" visible="false"/>
			<property qualifier="ImportCockpitCronJob.nextExecutionTime" sort-enabled="true" visible="false"/>
			<property qualifier="ImportCockpitCronJob.result" visible="false"/>
			<property qualifier="ImportCockpitCronJob.inputMedia" sort-enabled="true" visible="false"/>
		</group>
		<group name="advanced" visible="false">
			<label key="cockpit.config.label.Additional" />
			<property qualifier="ImportCockpitCronJob.errorMode" sort-enabled="true" visible="false"/>
			<property qualifier="ImportCockpitCronJob.logToDatabase" sort-enabled="true" visible="false"/>
			<property qualifier="ImportCockpitCronJob.logLevelDatabase" sort-enabled="true" visible="false"/>
			<property qualifier="ImportCockpitCronJob.logToFile" sort-enabled="true" visible="false"/>
			<property qualifier="ImportCockpitCronJob.logLevelFile" sort-enabled="true" visible="false"/>
		</group>
	    <group name="admin" visible="false">
	      <label key="config.general.administration" />
	      <property qualifier="item.pk" sort-enabled="false" visible="false"/>
	      <property qualifier="item.creationtime" sort-enabled="false" visible="false"/>
	      <property qualifier="item.modifiedtime" sort-enabled="false" visible="false"/>
	      <property qualifier="item.owner" sort-enabled="false" visible="false"/>
	    </group>
	</group>
</advanced-search>
