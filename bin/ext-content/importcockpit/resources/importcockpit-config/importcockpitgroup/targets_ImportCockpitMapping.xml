<targets>
	<types-whitelist>
		<type code="Product" />
		<type code="VariantProduct" />
		<type code="ProductReference" />
		<type code="Category" />
		<type code="Media" />
		<type code="MediaContainer" />
		<type code="UserGroup" />
		<type code="Customer" />
		<type code="Employee" />
		<type code="ClassificationSystem" />
		<type code="ClassificationSystemVersion" />
		<type code="ClassificationAttribute" />
		<type code="ClassificationAttributeUnit" />
		<type code="ClassAttributeAssignment" />
	</types-whitelist>

	<types-blacklist>
		<type code="Document" />
		<type code="Order" />
		<type code="LDIFMedia" />
		<type code="ConfigurationMedia" />
		<type code="CatalogVersionSyncScheduleMedia" />
		<type code="ImpExMedia" />
		<type code="LogFile" />
		<type code="JobMedia" />
		<type code="Formatter" />
		<type code="ImportCockpitMapping" />
		<type code="ImportCockpitInputMedia" />
		<type code="HeaderLibrary" />
		<type code="ProductFeature" />
	</types-blacklist>

	<attributes-blacklist>
		<property qualifier="Media.URL" />
		<property qualifier="Item.pk" />
		<property qualifier="Item.itemtype" />
		<property qualifier="Item.owner" />
		<property qualifier="Item.savedValues" />
		<property qualifier="Item.synchronizationSources" />
		<property qualifier="Item.synchronizedCopies" />
		<property qualifier="Item.creationtime" />
		<property qualifier="Item.modifiedtime" />
		<property qualifier="Item.assignedCockpitItemTemplates" />
		<property qualifier="Item.allDocuments" />
		<property qualifier="Category.catalogVersion" />
		<property qualifier="Product.catalogVersion" />
		<property qualifier="Media.catalogVersion" />
		<property qualifier="MediaContainer.catalogVersion" />
		<property qualifier="ClassificationAttribute.systemVersion" />
		<property qualifier="ClassificationAttributeUnit.systemVersion" />
		<property qualifier="ClassAttributeAssignment.systemVersion" />
	</attributes-blacklist>
</targets>
