<editor>
	<group qualifier="General" visible="true" initially-opened="true">
		<label key="cockpit.config.label.General" />
		<property qualifier="ImportCockpitCronJob.code" visible="true"/>
		<property qualifier="ImportCockpitCronJob.inputMedia" visible="true" editor="fileUploader"/>
		<property qualifier="ImportCockpitCronJob.status" visible="false" />
		<property qualifier="ImportCockpitCronJob.result" visible="false"/>
		<property qualifier="ImportCockpitCronJob.startTime" visible="false"/>
		<property qualifier="ImportCockpitCronJob.endTime" visible="false"/>
		<property qualifier="ImportCockpitCronJob.job" visible="false"/>
		<property qualifier="ImportCockpitCronJob.errorMode" visible="true"/>
	</group>
	<group qualifier="Mapping" visible="true" initially-opened="true">
		<label key="importcockpit.editorArea.mapping" />
		<property qualifier="ImportCockpitCronJob.mapping.catalogVersion" visible="true" editor="shortListEditor"/>
	</group>
	<group qualifier="Media" visible="true" initially-opened="true">
		<label key="importcockpit.editorArea.sourcefile" />
		<property qualifier="ImportCockpitCronJob.inputMedia.encoding" visible="true"/>
		<property qualifier="ImportCockpitCronJob.inputMedia.fieldSeparator" visible="true"/>
		<property qualifier="ImportCockpitCronJob.inputMedia.commentCharacter" visible="true"/>
		<property qualifier="ImportCockpitCronJob.inputMedia.quoteCharacter" visible="true"/>
		<property qualifier="ImportCockpitCronJob.inputMedia.linesToSkip" visible="true"/>
		<property qualifier="ImportCockpitCronJob.locale" visible="true" editor="localeEditor"/>
		<property qualifier="ImportCockpitCronJob.inputMedia.hasHeaderLine" visible="true"/>
	</group>
	<group qualifier="Timetable" visible="true" initially-opened="false">
		<label key="importcockpit.editorArea.timetable" />
		<property qualifier="ImportCockpitCronJob.triggers" visible="true" editor="triggerEditor">
			<parameter>
				<name>allowCreate</name>
				<value>true</value>
			</parameter>
		</property>
		<property qualifier="ImportCockpitCronJob.active" visible="true"/>
	</group>
	<group qualifier="Logging" visible="true" initially-opened="false">
		<label key="importcockpit.editorArea.logging" />
		<property qualifier="ImportCockpitCronJob.logToDatabase" visible="true"/>
		<property qualifier="ImportCockpitCronJob.logLevelDatabase" visible="true"/>
		<property qualifier="ImportCockpitCronJob.logToFile" visible="true" />
		<property qualifier="ImportCockpitCronJob.logLevelFile" visible="true"/>
	</group>
