   <type name="CatalogVersion" mode="append">
	<organizer>
		<editor>
		<tab name="tab.catalog.bmecat">
			<section name="section.bmecat" mode="append">
				<listlayout>
					<attribute name="importedVersions" labelwidth="100" width="224"/>
				</listlayout>
			</section>
		</tab>
		</editor>
	</organizer>
   </type>
   
   <type name="BMECatJobMedia" mode="append">
		<organizer>
			<search autoperform="true">
				<condition mode="delete" attribute="catalogVersion"/>
			</search>
			<result mode="append">
				<listview mode="replace">
					<itemlayout>
						<attribute name="creationtime" width="130" />
						<attribute name="code" width="300" />
					</itemlayout>
				</listview>
			</result>
		</organizer>
   </type>
   
