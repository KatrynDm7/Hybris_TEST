<?xml version="1.0" encoding="UTF-8"?>

<wizard-config showPrefilledValues="false" selectMode="true" createMode="true" displaySubtypes="true">
	<displayed-properties>
		<group qualifier="General" visible="true" initially-opened="true">
			<label key="config.general.mandatorydata" />
			<property qualifier="CMSLinkComponent.name"/>
			<property qualifier="CMSLinkComponent.linkName">
				<parameter name="defaultValue" value="ExternalLink"/>
			</property>
			<property qualifier="CMSLinkComponent.URL"/>
		</group>
		<group qualifier="FilledByWizard" visible="true" initially-opened="false">
			<label key="config.general.filledbywizard" />
			<property qualifier="CMSItem.uid"/>		
			<property qualifier="CMSItem.catalogVersion"/>		
			<property qualifier="CMSLinkComponent.external" visible="false">
				<parameter name="defaultValue" value="true"/>
			</property>
		</group>
	</displayed-properties>
</wizard-config>


