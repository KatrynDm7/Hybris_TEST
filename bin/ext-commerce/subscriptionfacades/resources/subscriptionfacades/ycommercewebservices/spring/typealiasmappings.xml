<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config/>


    <bean class="de.hybris.platform.commercefacades.xstream.alias.TypeAliasMapping">
        <property name="alias" value="product" />
        <property name="aliasedClass" value="de.hybris.platform.commercefacades.product.data.ProductData" />
    </bean>

    <bean class="de.hybris.platform.commercefacades.xstream.alias.TypeAliasMapping">
        <property name="alias" value="Image" />
        <property name="aliasedClass" value="de.hybris.platform.commercefacades.product.data.ImageData" />
    </bean>

 	<bean class="de.hybris.platform.commercefacades.xstream.alias.TypeAliasMapping">
		<property name="alias" value="subscriptionPricePlan" />
		<property name="aliasedClass" value="de.hybris.platform.subscriptionfacades.data.SubscriptionPricePlanData" />
	</bean> 

 	<bean class="de.hybris.platform.commercefacades.xstream.alias.TypeAliasMapping">
		<property name="alias" value="oneTimeChargeEntry" />
		<property name="aliasedClass" value="de.hybris.platform.subscriptionfacades.data.OneTimeChargeEntryData" />
	</bean> 

 	<bean class="de.hybris.platform.commercefacades.xstream.alias.TypeAliasMapping">
		<property name="alias" value="recurringChargeEntry" />
		<property name="aliasedClass" value="de.hybris.platform.subscriptionfacades.data.RecurringChargeEntryData" />
	</bean> 

 	<bean class="de.hybris.platform.commercefacades.xstream.alias.TypeAliasMapping">
		<property name="alias" value="perUnitUsageCharge" />
		<property name="aliasedClass" value="de.hybris.platform.subscriptionfacades.data.PerUnitUsageChargeData" />
	</bean> 

 	<bean class="de.hybris.platform.commercefacades.xstream.alias.TypeAliasMapping">
		<property name="alias" value="volumeUsageCharge" />
		<property name="aliasedClass" value="de.hybris.platform.subscriptionfacades.data.VolumeUsageChargeData" />
	</bean> 

 	<bean class="de.hybris.platform.commercefacades.xstream.alias.TypeAliasMapping">
		<property name="alias" value="tierUsageChargeEntry" />
		<property name="aliasedClass" value="de.hybris.platform.subscriptionfacades.data.TierUsageChargeEntryData" />
	</bean> 

 	<bean class="de.hybris.platform.commercefacades.xstream.alias.TypeAliasMapping">
		<property name="alias" value="overageUsageChargeEntry" />
		<property name="aliasedClass" value="de.hybris.platform.subscriptionfacades.data.OverageUsageChargeEntryData" />
	</bean> 

 	<bean class="de.hybris.platform.commercefacades.xstream.alias.TypeAliasMapping">
		<property name="alias" value="orderPrice" />
		<property name="aliasedClass" value="de.hybris.platform.subscriptionfacades.data.OrderPriceData" />
	</bean> 
		
 	<bean class="de.hybris.platform.commercefacades.xstream.alias.TypeAliasMapping">
		<property name="alias" value="orderEntryPrice" />
		<property name="aliasedClass" value="de.hybris.platform.subscriptionfacades.data.OrderEntryPriceData" />
	</bean>
	           
</beans>
