#################################################################
# Form Sample - Contact Form
#################################################################
INSERT_UPDATE YFormDefinition	;applicationId[unique=true]	;formId[unique=true]	;version[unique=true]	;documentId											;title				;description							;content
										;yforms								;contact					;1								;66ba893a657df5595c228bc60c0a195baa5e8be6	;"Contact Form"	;"Sample Form for Contact Data"	;"
<xh:html xmlns:xh=""http://www.w3.org/1999/xhtml"" xmlns:xf=""http://www.w3.org/2002/xforms""
         xmlns:xs=""http://www.w3.org/2001/XMLSchema""
         xmlns:xsi=""http://www.w3.org/2001/XMLSchema-instance""
         xmlns:ev=""http://www.w3.org/2001/xml-events""
         xmlns:xi=""http://www.w3.org/2001/XInclude""
         xmlns:xxi=""http://orbeon.org/oxf/xml/xinclude""
         xmlns:xxf=""http://orbeon.org/oxf/xml/xforms""
         xmlns:exf=""http://www.exforms.org/exf/1-0""
         xmlns:fr=""http://orbeon.org/oxf/xml/form-runner""
         xmlns:saxon=""http://saxon.sf.net/""
         xmlns:sql=""http://orbeon.org/oxf/xml/sql""
         xmlns:soap=""http://schemas.xmlsoap.org/soap/envelope/""
         xmlns:fb=""http://orbeon.org/oxf/xml/form-builder"">
    <xh:head>
        <xh:title>Contact Form</xh:title>
        <xf:model id=""fr-form-model"" xxf:expose-xpath-types=""true"">

            <!-- Main instance -->
            <xf:instance id=""fr-form-instance"">
                <form>
                    <contact>
                        <name/>
                        <surname/>
                        <email/>
                        <phone/>
                        <callmeback/>
                        <message/>
                    </contact>
                </form>
            </xf:instance>

            <!-- Bindings -->
            <xf:bind id=""fr-form-binds"" ref=""instance('fr-form-instance')"">
                <xf:bind id=""contact-bind"" name=""contact"" ref=""contact"">
                    <xf:bind id=""name-bind"" name=""name"" ref=""name"" required=""true()""
                             constraint=""string-length(.) &gt;= 2 and string-length(.) &lt;= 50""/>
                    <xf:bind id=""surname-bind"" ref=""surname"" name=""surname"" required=""true()""
                             constraint=""string-length(.) &gt;=  2 and string-length(.) &lt;= 50""/>
                    <xf:bind id=""phone-bind"" ref=""phone"" name=""phone""
                             constraint=""(string(/form/contact/callmeback) = 'true' and (string-length(.) &gt;= 5 and string-length(.) &lt;= 15)) or string(/form/contact/callmeback) = '' or string(/form/contact/callmeback) = 'false'""/>
                    <xf:bind id=""email-bind"" ref=""email"" name=""email"" required=""true()"">
                        <xf:constraint id=""constraint-8-constraint""
                                       value=""matches(upper-case(.),'^[A-Z0-9._%+-]+@[A-Z0-9.-]+\.[A-Z]{2,4}$')""
                                       level=""error""/>
                        <xf:constraint id=""constraint-9-constraint"" value=""string-length(.) &lt;= 100"" level=""error""/>
                    </xf:bind>
                    <xf:bind id=""callmeback-bind"" ref=""callmeback"" name=""callmeback"" type=""xf:boolean""/>
                    <xf:bind id=""message-bind"" ref=""message"" name=""message""
                             constraint=""string-length(.) &gt;= 20 and string-length(.) &lt;= 500""
                             required=""true()""/>
                </xf:bind>
            </xf:bind>

            <!-- Metadata -->
            <xf:instance xxf:readonly=""true"" id=""fr-form-metadata"">
                <metadata>
                    <application-name>yforms</application-name>
                    <form-name>contact</form-name>
                    <title xml:lang=""en"">Contact Form</title>
                    <description xml:lang=""en"">Sample Form for Contact Data</description>
                </metadata>
            </xf:instance>

            <!-- Attachments -->
            <xf:instance id=""fr-form-attachments"">
                <attachments>
                    <css mediatype=""text/css"" filename="""" size=""""/>
                    <pdf mediatype=""application/pdf"" filename="""" size=""""/>
                </attachments>
            </xf:instance>

            <!-- All form resources -->
            <!-- Don't make readonly by default in case a service modifies the resources -->
            <xf:instance id=""fr-form-resources"" xxf:readonly=""false"">
                <resources>
                    <resource xml:lang=""en"">
                        <contact>
                            <label>Contact</label>
                        </contact>
                        <name>
                            <label>Name</label>
                            <hint/>
                            <alert>Field needs at least 2 characters and can have at most 50.</alert>
                        </name>
                        <surname>
                            <label>Surname</label>
                            <hint/>
                            <alert>Field needs at least 2 characters and can have at most 50.</alert>
                        </surname>
                        <phone>
                            <label>Phone Number</label>
                            <hint/>
                            <alert>Phone number needs at least 5 characters and can have at most 15.</alert>
                        </phone>
                        <callmeback>
                            <label>Call me back!</label>
                            <hint/>
                        </callmeback>
                        <message>
                            <label>Message</label>
                            <hint/>
                            <alert>Field needs at least 20 characters and can have at most 500.</alert>
                        </message>
                        <email>
                            <label>Email</label>
                            <hint/>
                            <alert>Email address does not seem to be a valid one</alert>
                            <alert>Field can have at most 100 characters.</alert>
                            
                        </email>
                    </resource>
                </resources>
            </xf:instance>

            <!-- Utility instances for services -->
            <xf:instance id=""fr-service-request-instance"" xxf:exclude-result-prefixes=""#all"">
                <request/>
            </xf:instance>

            <xf:instance id=""fr-service-response-instance"" xxf:exclude-result-prefixes=""#all"">
                <response/>
            </xf:instance>

        </xf:model>
    </xh:head>
    <xh:body>
        <fr:view>
