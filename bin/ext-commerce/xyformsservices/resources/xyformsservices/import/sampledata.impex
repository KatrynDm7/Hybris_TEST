                </form>
            </xf:instance>

            <!-- Bindings -->
            <xf:bind id=""fr-form-binds"" ref=""instance('fr-form-instance')"">
                <xf:bind id=""contact-bind"" name=""contact"" ref=""contact"">
                    <xf:bind id=""name-bind"" name=""name"" ref=""name"" required=""true()""
                             constraint=""string-length(.) &gt;= 2 and string-length(.) &lt;= 50""/>
                    <xf:bind id=""surname-bind"" ref=""surname"" name=""surname"" required=""true()""
                             constraint=""string-length(.) &gt;=  2 and string-length(.) &lt;= 50""/>
                    <xf:bind id=""phone-bind"" ref=""phone"" name=""phone""
                             constraint=""(string(/form/contact/callmeback) = 'true' and (string-length(.) &gt;= 5 and string-length(.) &lt;= 15)) or string(/form/contact/callmeback) = '' or string(/form/contact/callmeback) = 'false'""/>
