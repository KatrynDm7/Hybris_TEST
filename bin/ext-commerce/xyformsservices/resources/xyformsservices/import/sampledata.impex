                    <pdf mediatype=""application/pdf"" filename="""" size=""""/>
                </attachments>
            </xf:instance>

            <!-- All form resources -->
            <!-- Don't make readonly by default in case a service modifies the resources -->
            <xf:instance id=""fr-form-resources"" xxf:readonly=""false"">
                <resources>
                    <resource xml:lang=""en"">
                        <contact>
                            <label>Contact</label>
                        </contact>
                        <name>
                            <label>Name</label>
                            <hint/>
                            <alert>Field needs at least 2 characters and can have at most 50.</alert>
                        </name>
                        <surname>
                            <label>Surname</label>
                            <hint/>
                            <alert>Field needs at least 2 characters and can have at most 50.</alert>
