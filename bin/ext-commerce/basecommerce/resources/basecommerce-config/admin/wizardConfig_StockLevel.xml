<?xml version="1.0" encoding="UTF-8"?>

<wizard-config showPrefilledValues="true" selectMode="true" createMode="true" displaySubtypes="false" createWithinPopup="false" createWithinEditor="false">
	<displayed-properties>			
	
		<group qualifier="General" visible="true">
			<label key="cockpit.config.label.General" />
			<!-- required fields -->
			<property qualifier="StockLevel.warehouse" visible="true" editorCode="shortListEditor"/>
			<property qualifier="StockLevel.productCode" visible="true" editorCode="productCodeEditor"/>
		</group>
		<group qualifier="StockLevel" initially-opened="true">
			<label key="basecommerce.wizardConfig.stocklevelinformation" />
			<property qualifier="StockLevel.available"/>
			<property qualifier="StockLevel.reserved"/>
		</group>
		<group qualifier="StockSettings" initially-opened="true"> 
			<label key="basecommerce.wizardConfig.stocklevelsettings" />
			<property qualifier="StockLevel.inStockStatus"/>			
			<property qualifier="StockLevel.overSelling"/>
		</group>														
	</displayed-properties>
</wizard-config>


