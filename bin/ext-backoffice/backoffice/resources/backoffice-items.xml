<items 	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			xsi:noNamespaceSchemaLocation="items.xsd">
	<itemtypes>
		<itemtype code="BackofficeRole" extends="UserGroup"
			jaloclass="com.hybris.backoffice.jalo.user.BackofficeRole" autocreate="true" generate="true" >
			<attributes>
				<attribute type="StringCollection" qualifier="authorities" >
					<modifiers read="true" write="true" />
					<persistence type="property"/>
				</attribute>
			</attributes>
		</itemtype>

		<itemtype code="Principal" autocreate="false" generate="false">
			<attributes>
				<attribute qualifier="backOfficeLoginDisabled" type="java.lang.Boolean">
					<modifiers read="true" write="true" search="true" optional="true"/>
					<persistence type="property"/>
				</attribute>
			</attributes>
		</itemtype>
	</itemtypes>
</items>
