					<xf:bind id=""phone-bind"" ref=""phone"" name=""phone"" required=""true()"">
						<xf:constraint id=""constraint-50-constraint""
							value=""string-length(.) &lt; 1 or matches(., '^[0-9A-Za-z]+$')""
							level=""error"" />
						<xf:constraint id=""constraint-51-constraint""
							value=""string-length(.) &gt;= 7 and string-length() &lt;= 20""
							level=""error"" />
					</xf:bind>
					<xf:bind id=""email-bind"" ref=""email"" name=""email"" required=""true()"">
						<xf:constraint id=""constraint-52-constraint""
