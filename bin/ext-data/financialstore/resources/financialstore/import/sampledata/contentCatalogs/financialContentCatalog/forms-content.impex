<xh:html xmlns:xh=""http://www.w3.org/1999/xhtml"" xmlns:ev=""http://www.w3.org/2001/xml-events"" xmlns:exf=""http://www.exforms.org/exf/1-0"" xmlns:fb=""http://orbeon.org/oxf/xml/form-builder"" xmlns:fr=""http://orbeon.org/oxf/xml/form-runner"" xmlns:saxon=""http://saxon.sf.net/"" xmlns:soap=""http://schemas.xmlsoap.org/soap/envelope/"" xmlns:sql=""http://orbeon.org/oxf/xml/sql"" xmlns:xf=""http://www.w3.org/2002/xforms"" xmlns:xi=""http://www.w3.org/2001/XInclude"" xmlns:xs=""http://www.w3.org/2001/XMLSchema"" xmlns:xsi=""http://www.w3.org/2001/XMLSchema-instance"" xmlns:xxf=""http://orbeon.org/oxf/xml/xforms"" xmlns:xxi=""http://orbeon.org/oxf/xml/xinclude"">
	<xh:head>
		<xh:title>Event Insurance</xh:title>
		<xf:model id=""fr-form-model"" xxf:expose-xpath-types=""true"">
			<xf:instance id=""fr-form-instance"">
				<form>
					<eventDetails>
                        <country />
                        <date />
                        <venue />
                        <address />
                        <city />
					    <first-name />
					    <surname />
					    <email />
						<ciAddressL1 />
						<ciAddressL2 />
						<ciPostcode />
						<ciCity />
					</eventDetails>
				</form>
			</xf:instance>
			<xf:bind xmlns:dataModel=""java:org.orbeon.oxf.fb.DataModel"" id=""fr-form-binds"" ref=""instance('fr-form-instance')"">
				<xf:bind id=""eventDetails-bind"" ref=""eventDetails"" name=""eventDetails"">
                    <xf:bind id=""country-bind"" ref=""country"" name=""country"" required=""true()"" />
                    <xf:bind id=""date-bind"" ref=""date"" name=""date"" type=""xs:date"" required=""true()"">
                        <xf:constraint id=""constraint-31-constraint"" value="". &gt; current-date()"" level=""error"" />
                        <xf:constraint id=""constraint-32-constraint"" value="".&lt;= current-date() + xs:yearMonthDuration('P1Y')"" level=""error"" />
                    </xf:bind>
                    <xf:bind id=""venue-bind"" ref=""venue"" name=""venue"" required=""true()"">
                        <xf:constraint id=""constraint-33-constraint"" value=""string-length(.)&lt;=40"" level=""error"" />
                        <xf:constraint id=""constraint-35-constraint"" value=""string-length(.)&gt;0"" level=""error"" />
                    </xf:bind>
                    <xf:bind id=""city-bind"" ref=""city"" name=""city"" required=""true()"">
                        <xf:constraint id=""constraint-38-constraint"" value=""string-length(.) &lt;=40"" level=""error"" />
                        <xf:constraint id=""constraint-39-constraint"" value=""string-length(.)&gt;0"" level=""error"" />
                    </xf:bind>
                    <xf:bind id=""address-bind"" ref=""address"" name=""address"" required=""true()"">
                        <xf:constraint id=""constraint-36-constraint"" value=""string-length(.) &lt;=40"" level=""error"" />
                        <xf:constraint id=""constraint-37-constraint"" value=""string-length(.)&gt;0"" level=""error"" />
                    </xf:bind>
					<xf:bind id=""ciPostcode-bind"" ref=""ciPostcode"" name=""ciPostcode"" required=""true()"" />
					<xf:bind id=""ciCity-bind"" ref=""ciCity"" name=""ciCity"" required=""true()"">
						<xf:constraint id=""constraint-30-constraint"" value=""string-length(.) &lt;=40"" level=""error"" />
						<xf:constraint id=""constraint-55-constraint"" value=""string-length(.)&gt;0"" level=""error"" />
					</xf:bind>
					<xf:bind id=""ciAddressL1-bind"" ref=""ciAddressL1"" name=""ciAddressL1"" required=""true()"">
						<xf:constraint id=""constraint-22-constraint"" value=""string-length(.) &lt;=40"" level=""error"" />
						<xf:constraint id=""constraint-23-constraint"" value=""string-length(.)&gt;0"" level=""error"" />
					</xf:bind>
					<xf:bind id=""ciAddressL2-bind"" ref=""ciAddressL2"" name=""ciAddressL2"">
						<xf:constraint id=""constraint-25-constraint"" value=""string(.)='' or (string-length(.) &lt;=40)"" level=""error"" />
