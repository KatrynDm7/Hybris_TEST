				<xf:bind id=""personal-details-bind"" name=""personal-details""
					ref=""personal-details"">
					<xf:bind id=""first-name-bind"" ref=""first-name"" name=""first-name""
						required=""true()"">
						<xf:constraint id=""validation-49-validation""
							value=""string-length(.) &gt;= 2 and string-length() &lt;= 50""
							level=""error"" />
					</xf:bind>
					<xf:bind id=""last-name-bind"" ref=""last-name"" name=""last-name""
						required=""true()"">
						<xf:constraint id=""validation-50-validation""
