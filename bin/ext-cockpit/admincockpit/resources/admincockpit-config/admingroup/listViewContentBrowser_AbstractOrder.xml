<?xml version="1.0" encoding="UTF-8"?>
<list-view unassigned-group-name="Other">
	<group name="General">
		<custom
			class="de.hybris.platform.cockpit.services.config.impl.LineNumberColumn"
			name="Line nr" visible="true" selectable="true">
			<label key="config.general.linenr" />
		</custom>
		<property qualifier="AbstractOrder.code" visible="true" />
		<property qualifier="AbstractOrder.status" visible="true"/>
		<property qualifier="AbstractOrder.date" visible="true" />
		<property qualifier="AbstractOrder.currency" visible="true" />
		
		<property qualifier="AbstractOrder.totalPrice" visible="true"/>
		<property qualifier="AbstractOrder.user" visible="true"/>
		<property qualifier="AbstractOrder.deliveryStatus" visible="true"/>
		<property qualifier="AbstractOrder.deliveryAddress" visible="true"/>
		
		<custom spring-bean="OrderContentBrowserActionColumn" name="Actions"
			visible="true" selectable="true">
			<label key="config.general.actions" />
		</custom>
	</group>
	
</list-view>
