<items 	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
			xsi:noNamespaceSchemaLocation="items.xsd">

	<itemtypes>


		<itemtype code="SAPConfiguration" autocreate="false" generate="false">
		    <attributes>
		    
		    	<attribute qualifier="sapordermgmt_enabled"  type="boolean" autocreate="true">
					<persistence type="property" />
					<modifiers optional="false" />
					<description>Determines whether the site should use external external order pricing</description>
				</attribute>
		        <attribute qualifier="sapordermgmt_headerCondTypeFreight" type="java.lang.String" generate="true">
		            <description>sapordermgmt_headerCondTypeFreight</description>
		            <persistence type="property" />
		        </attribute>
		        <attribute qualifier="sapordermgmt_sourceFreight" type="java.lang.String" generate="true">
		            <description>sapordermgmt_sourceFreight</description>
		            <persistence type="property" />
		        </attribute>
		        <attribute qualifier="sapordermgmt_sourceNetWOFreight" type="java.lang.String" generate="true">
		            <description>sapordermgmt_sourceNetWOFreight</description>
		            <persistence type="property" />
		        </attribute>
		        <attribute qualifier="sapordermgmt_maxHits" type="java.lang.String" generate="true">
		            <description>sapordermgmt_maxHits</description>
		            <persistence type="property" />
					<defaultvalue>"100"</defaultvalue>		            
		        </attribute>
		        <attribute qualifier="sapordermgmt_dateRange" type="java.lang.String" generate="true">
		            <description>sapordermgmt_dateRange</description>
		            <persistence type="property" />
					<defaultvalue>"365"</defaultvalue>		            
		        </attribute>
		    </attributes>
		</itemtype>


	</itemtypes>
</items>
