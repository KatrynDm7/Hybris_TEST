<?xml version="1.0" encoding="ISO-8859-1"?>
<configuration xmlns="hybris.de/schemas/hmc/generic"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="hybris.de/schemas/hmc/generic hmc.xsd">

	<type name="SAPConfiguration" mode="append" >
		<organizer>
			<editor>
				<tab name="tab.sapordermgmt">
					<section name="section.sapordermgmt">
						<listlayout>
								<attribute name="sapordermgmt_enabled"></attribute>
								<attribute name="sapordermgmt_headerCondTypeFreight" ></attribute>
								<attribute name="sapordermgmt_sourceFreight"></attribute>
								<attribute name="sapordermgmt_sourceNetWOFreight"></attribute>
						</listlayout>
					</section>
					<section name="section.sapordermgmtsearch">
						<listlayout>
								<attribute name="sapordermgmt_maxHits"></attribute>
								<attribute name="sapordermgmt_dateRange"></attribute>
						</listlayout>
					</section>
				</tab>
			</editor>
		</organizer>
	</type>

</configuration>